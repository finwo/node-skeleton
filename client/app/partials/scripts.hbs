<script src="/assets/js/lib/req.js"></script>
<script>
    window._lang        = "{{_lang}}";
    window.translations = { {{#each _config.language}}"{{@key}}":{ {{#each this}}"{{@key}}": "{{this}}",{{/each}} },{{/each}} };
    require.baseUri = '/assets/';
    require.paths   = {
        'api'             : 'js/lib/api.js',
        'app'             : 'js/app.js',
        'bluebird'        : 'bower/bluebird/js/browser/bluebird.min.js',
        'cachep2p'        : 'https://unpkg.com/cachep2p/cachep2p.min.js',
        'data-script'     : 'js/helpers/data-script.js',
        'jquery'          : 'bower/jquery/dist/jquery.min.js',
        'jquery-watch-dom': 'bower/jquery-watch-dom/jquery-watch.min.js',
        'notify'          : 'bower/js-notify/notify.min.js',
        'sockjs'          : 'bower/sockjs-client/dist/sockjs.min.js',
        'translate'       : 'js/lib/translate.js',
        'uid'             : 'js/lib/uid.js'
    };
    require.getter['data-script']      = function() { return true;                    };
    require.getter['jquery-watch-dom'] = function() { return $ && $.fn && $.fn.watch; };
    require(['cachep2p'], function(CacheP2P) {
        var c = new CacheP2P();
        c.on('ready', function() { require(['app', 'data-script'], function(){}); });
    });
</script>
